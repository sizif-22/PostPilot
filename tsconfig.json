{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/channels",
    "src/app/channels/[id]"
, "src/app/instagram/oldpage", "src/components/DashboardComponents/Calendar/ContinuousCalendar.tsxo", "src/components/DashboardComponents/Calendar/DetailsDialog.tsxo", "src/app/subLayout"  ],
  "exclude": [
    "node_modules"
  ]
}
